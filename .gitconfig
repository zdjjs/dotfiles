[include]
	path = ~/.gitlocal
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
	precomposeunicode = true
	quotepath = false
	autoCRLF = false
[push]
	default = simple
[alias]
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[diff]
	compactionHeuristic = true
