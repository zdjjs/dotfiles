[include]
	path = ~/.gitlocal
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
	precomposeunicode = true
	quotepath = false
	autoCRLF = false
[push]
	default = simple
[alias]
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	del-branch = !git branch --merged | grep -vE '^\\*|master$|develop$' | xargs -I % git branch -d %
[diff]
	compactionHeuristic = true
[user]
	signingkey = E1FA9FF1924410E9BE8EFED5BEFAD0281680EC36
[commit]
	gpgsign = true
